# API Keys - replace with your actual keys
OPENAI_API_KEY=your-openai-key-here
ANTHROPIC_API_KEY=your-anthropic-key-here

# Configuration Options
OPENLIT_ENDPOINT=http://127.0.0.1:4318

# Database Connections
SQL_DB_CONNECTION=postgresql://user:password@localhost:5432/dbname

# MCP-SQL-Agent PostgreSQL Configuration
POSTGRES_USER=postgres
POSTGRES_PASSWORD=postgres
POSTGRES_DB=testdb
POSTGRES_HOST=localhost
POSTGRES_PORT=5432
POSTGRES_READONLY_USER=readonly
POSTGRES_READONLY_PASSWORD=readonly

# MCP-SQL-Agent Snowflake Configuration
SNOWFLAKE_ACCOUNT=your-account.us-east-1
SNOWFLAKE_USER=your-user
SNOWFLAKE_PASSWORD=your-password
SNOWFLAKE_READONLY_USER=readonly_user
SNOWFLAKE_READONLY_PASSWORD=readonly_password
SNOWFLAKE_DATABASE=your-database
SNOWFLAKE_SCHEMA=public
SNOWFLAKE_WAREHOUSE=compute_wh
SNOWFLAKE_ROLE=accountadmin
SNOWFLAKE_READONLY_ROLE=readonly_role

# MCP-SQL-Agent Grafana Configuration
GRAFANA_USER=admin
GRAFANA_PASSWORD=admin
GRAFANA_API_KEY=your-grafana-api-key

# Other Settings
LOG_LEVEL=INFO
DEBUG=false